
.row
  %h2.ollert-header
    .text-center.col-xs-12
      NUEVO PROYECTO de #{@orgName}
.row
  %form#components.form-horizontal{role: "form"}
    %fieldset
      .tab-content
        / Tabs of snippets go here
        #input.tab-pane.active
          .component
            / Text input
            .form-group
              %label.col-md-4.control-label{for: "textinput"} Text Input
              .col-md-4
                %input#textinput.form-control.input-md{name: "textinput", placeholder: "placeholder", style: "cursor: auto; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;", type: "text"}/
                %span.help-block help
          .component
            / Password input
            .form-group
              %label.col-md-4.control-label{for: "passwordinput"} Password Input
              .col-md-4
                %input#passwordinput.form-control.input-md{name: "passwordinput", placeholder: "placeholder", style: "background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;", type: "password"}/
                %span.help-block help
          .component
            / Search input
            .form-group
              %label.col-md-4.control-label{for: "searchinput"} Search Input
              .col-md-4
                %input#searchinput.form-control.input-md{name: "searchinput", placeholder: "placeholder", type: "search"}/
                %p.help-block help
          .component
            / Prepended text
            .form-group
              %label.col-md-4.control-label{for: "prependedtext"} Prepended Text
              .col-md-4
                .input-group
                  %span.input-group-addon prepend
                  %input#prependedtext.form-control{name: "prependedtext", placeholder: "placeholder", type: "text"}/
                %p.help-block help
          .component
            / Appended Input
            .form-group
              %label.col-md-4.control-label{for: "appendedtext"} Appended Text
              .col-md-4
                .input-group
                  %input#appendedtext.form-control{name: "appendedtext", placeholder: "placeholder", type: "text"}/
                  %span.input-group-addon append
                %p.help-block help
          .component
            / Prepended checkbox
            .form-group
              %label.col-md-4.control-label{for: "prependedcheckbox"} Prepended Checkbox
              .col-md-4
                .input-group
                  %span.input-group-addon
                    %input{type: "checkbox"}/
                  %input#prependedcheckbox.form-control{name: "prependedcheckbox", placeholder: "placeholder", type: "text"}/
                %p.help-block help
          .component
            / Appended checkbox
            .form-group
              %label.col-md-4.control-label{for: "appendedcheckbox"} Appended Checkbox
              .col-md-4
                .input-group
                  %input#appendedcheckbox.form-control{name: "appendedcheckbox", placeholder: "placeholder", type: "text"}/
                  %span.input-group-addon
                    %input{type: "checkbox"}/
                %p.help-block help
          .component
            / Button Drop Down
            .form-group
              %label.col-md-4.control-label{for: "buttondropdown"} Button Drop Down
              .col-md-4
                .input-group
                  %input#buttondropdown.form-control{name: "buttondropdown", placeholder: "placeholder", type: "text"}/
                  .input-group-btn
                    %button.btn.btn-default.dropdown-toggle{"data-toggle" => "dropdown", type: "button"}
                      Action
                      %span.caret
                    %ul.dropdown-menu.pull-right
                      %li
                        %a{href: "#"} Option one
                      %li
                        %a{href: "#"} Option two
                      %li
                        %a{href: "#"} Option three
          .component
            / Textarea
            .form-group
              %label.col-md-4.control-label{for: "textarea"} Text Area
              .col-md-4
                %textarea#textarea.form-control{name: "textarea"} default text
        #radioscheckboxes.tab-pane
          .component
            / Multiple Radios
            .form-group
              %label.col-md-4.control-label{for: "radios"} Multiple Radios
              .col-md-4
                .radio
                  %label{for: "radios-0"}
                    %input#radios-0{checked: "checked", name: "radios", type: "radio", value: "1"}/
                    Option one
                .radio
                  %label{for: "radios-1"}
                    %input#radios-1{name: "radios", type: "radio", value: "2"}/
                    Option two
          .component
            / Multiple Radios (inline)
            .form-group
              %label.col-md-4.control-label{for: "radios"} Inline Radios
              .col-md-4
                %label.radio-inline{for: "radios-0"}
                  %input#radios-0{checked: "checked", name: "radios", type: "radio", value: "1"}/
                  1
                %label.radio-inline{for: "radios-1"}
                  %input#radios-1{name: "radios", type: "radio", value: "2"}/
                  2
                %label.radio-inline{for: "radios-2"}
                  %input#radios-2{name: "radios", type: "radio", value: "3"}/
                  3
                %label.radio-inline{for: "radios-3"}
                  %input#radios-3{name: "radios", type: "radio", value: "4"}/
                  4
          .component
            / Multiple Checkboxes
            .form-group
              %label.col-md-4.control-label{for: "checkboxes"} Multiple Checkboxes
              .col-md-4
                .checkbox
                  %label{for: "checkboxes-0"}
                    %input#checkboxes-0{name: "checkboxes", type: "checkbox", value: "1"}/
                    Option one
                .checkbox
                  %label{for: "checkboxes-1"}
                    %input#checkboxes-1{name: "checkboxes", type: "checkbox", value: "2"}/
                    Option two
          .component
            / Multiple Checkboxes (inline)
            .form-group
              %label.col-md-4.control-label{for: "checkboxes"} Inline Checkboxes
              .col-md-4
                %label.checkbox-inline{for: "checkboxes-0"}
                  %input#checkboxes-0{name: "checkboxes", type: "checkbox", value: "1"}/
                  1
                %label.checkbox-inline{for: "checkboxes-1"}
                  %input#checkboxes-1{name: "checkboxes", type: "checkbox", value: "2"}/
                  2
                %label.checkbox-inline{for: "checkboxes-2"}
                  %input#checkboxes-2{name: "checkboxes", type: "checkbox", value: "3"}/
                  3
                %label.checkbox-inline{for: "checkboxes-3"}
                  %input#checkboxes-3{name: "checkboxes", type: "checkbox", value: "4"}/
                  4

 